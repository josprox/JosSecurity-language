{
  "name": "JosSecurity Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editorLineNumber.foreground": "#858585",
    "editorCursor.foreground": "#00ff00",
    "editor.selectionBackground": "#264f78",
    "editor.lineHighlightBackground": "#2a2a2a"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.control.joss", "keyword.other.joss"],
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.type.joss"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#569CD6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable.other.joss"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": ["entity.name.function.joss", "support.function.joss"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": ["entity.name.type.class.joss", "support.class.joss"],
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": ["keyword.operator.ternary.joss"],
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "bold"
      }
    }
  ]
}

{
  "Class": {
    "prefix": "class",
    "body": [
      "class ${1:ClassName} {",
      "\tInit ${2:main}() {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "Create a new class with Init method"
  },
  "Controller": {
    "prefix": "controller",
    "body": [
      "class ${1:Name}Controller {",
      "\tfunc index() {",
      "\t\treturn View::render(\"${2:view}\", {\"title\": \"${3:Title}\"})",
      "\t}",
      "}"
    ],
    "description": "Create a new Controller class"
  },
  "Init Method": {
    "prefix": "init",
    "body": [
      "Init ${1:methodName}() {",
      "\t$0",
      "}"
    ],
    "description": "Create an Init method"
  },
  "Router Get": {
    "prefix": "route:get",
    "body": [
      "Router::get(\"/${1:path}\", \"${2:Controller}@${3:method}\")"
    ],
    "description": "Define a GET route"
  },
  "Router Post": {
    "prefix": "route:post",
    "body": [
      "Router::post(\"/${1:path}\", \"${2:Controller}@${3:method}\")"
    ],
    "description": "Define a POST route"
  },
  "Router Match": {
    "prefix": "route:match",
    "body": [
      "Router::match(\"${1:GET|POST}\", \"/${2:path}\", \"${3:Controller}@${4:method}\")"
    ],
    "description": "Define a multi-method route"
  },
  "Router Group": {
    "prefix": "route:group",
    "body": [
      "Router::group(\"/${1:prefix}\")",
      "$0",
      "Router::end()"
    ],
    "description": "Define a router group"
  },
  "Router Put": {
    "prefix": "route:put",
    "body": [
      "Router::put(\"/${1:path}\", \"${2:Controller}@${3:method}\")"
    ],
    "description": "Define a PUT route"
  },
  "Router Delete": {
    "prefix": "route:delete",
    "body": [
      "Router::delete(\"/${1:path}\", \"${2:Controller}@${3:method}\")"
    ],
    "description": "Define a DELETE route"
  },
  "Router API": {
    "prefix": "route:api",
    "body": [
      "Router::api(\"/${1:path}\", \"${2:Controller}@${3:method}\")"
    ],
    "description": "Define an API route (GET & POST)"
  },
  "Redirect": {
    "prefix": "redirect",
    "body": [
      "return redirect(\"${1:url}\")"
    ],
    "description": "Redirect to URL"
  },
  "Auth Check": {
    "prefix": "auth:check",
    "body": [
      "if (Auth::check()) {",
      "\t$0",
      "}"
    ],
    "description": "Check if user is authenticated"
  },
  "Auth Guest": {
    "prefix": "auth:guest",
    "body": [
      "if (Auth::guest()) {",
      "\t$0",
      "}"
    ],
    "description": "Check if user is guest"
  },
  "Auth Attempt": {
    "prefix": "auth:attempt",
    "body": [
      "if (Auth::attempt($email, $password)) {",
      "\treturn Response::redirect(\"/dashboard\")",
      "}"
    ],
    "description": "Attempt login"
  },
  "Ternary": {
    "prefix": "tern",
    "body": [
      "(${1:condition}) ? {",
      "\t${2:// true}",
      "} : {",
      "\t${3:// false}",
      "}"
    ],
    "description": "Ternary operator with blocks"
  },
  "If (Ternary)": {
    "prefix": "if",
    "body": [
      "(${1:condition}) ? {",
      "\t$0",
      "}"
    ],
    "description": "If statement (Ternary)"
  },
  "If Else (Ternary)": {
    "prefix": "ifelse",
    "body": [
      "(${1:condition}) ? {",
      "\t${2:// true}",
      "} : {",
      "\t$0",
      "}"
    ],
    "description": "If-Else statement (Ternary)"
  },
  "Else (Ternary)": {
    "prefix": "else",
    "body": [
      "} : {",
      "\t$0",
      "}"
    ],
    "description": "Else block (Ternary)"
  },
  "Switch (Ternary Ladder)": {
    "prefix": "switch",
    "body": [
      "(${1:var} == ${2:value1}) ? {",
      "\t${3:// case 1}",
      "} : (${1:var} == ${4:value2}) ? {",
      "\t${5:// case 2}",
      "} : {",
      "\t${6:// default}",
      "}"
    ],
    "description": "Switch statement (Ternary Ladder)"
  },
  "Ternary Inline": {
    "prefix": "terni",
    "body": [
      "${1:variable} = (${2:condition}) ? ${3:trueValue} : ${4:falseValue}"
    ],
    "description": "Inline ternary assignment"
  },
  "Foreach": {
    "prefix": "foreach",
    "body": [
      "foreach (${1:array} as ${2:item}) {",
      "\t$0",
      "}"
    ],
    "description": "Foreach loop"
  },
  "Print": {
    "prefix": "print",
    "body": [
      "print(${1:\"message\"})"
    ],
    "description": "Print statement"
  },
  "Variable": {
    "prefix": "var",
    "body": [
      "${1:type} \\$${2:name} = ${3:value}"
    ],
    "description": "Declare a variable"
  },
  "Auth Create": {
    "prefix": "authcreate",
    "body": [
      "\\$auth = new Auth()",
      "\\$auth.create([${1:email}, ${2:password}, ${3:name}])"
    ],
    "description": "Create user with Auth"
  },
  "GranMySQL Query": {
    "prefix": "dbquery",
    "body": [
      "\\$db = new GranMySQL()",
      "\\$db.table(\"${1:tableName}\")",
      "\\$db.where(\"${2:column}\", ${3:value})",
      "\\$result = \\$db.get()"
    ],
    "description": "Query database with GranMySQL"
  },
  "GranMySQL Pagination": {
    "prefix": "dbpaginate",
    "body": [
      "\\$db = new GranMySQL()",
      "\\$page = Request::input(\"page\")",
      "\\$perPage = ${1:10}",
      "\\$offset = (Math.abs(\\$page) - 1) * \\$perPage",
      "\\$total = \\$db.table(\"${2:tableName}\").count()",
      "\\$results = \\$db.table(\"${2:tableName}\").orderBy(\"id\", \"DESC\").limit(\\$perPage).offset(\\$offset).get()"
    ],
    "description": "Paginate database results"
  },
  "GranMySQL Insert": {
    "prefix": "dbinsert",
    "body": [
      "\\$db = new GranMySQL()",
      "\\$db.table(\"${1:tableName}\")",
      "\\$db.insert([${2:columns}], [${3:values}])"
    ],
    "description": "Insert into database"
  },
  "Comment Block": {
    "prefix": "comment",
    "body": [
      "// ===== ${1:SECTION} =====",
      "$0"
    ],
    "description": "Section comment"
  },
  "Main Class": {
    "prefix": "main",
    "body": [
      "class Main {",
      "\tInit main() {",
      "\t\tprint(\"${1:Hello JosSecurity}\")",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "Main class entry point"
  },
  "Async Task": {
    "prefix": "async",
    "body": [
      "\\$${1:future} = async(${2:expression})",
      "\\$${3:result} = await(\\$${1:future})"
    ],
    "description": "Create async task with await"
  },
  "Async Function": {
    "prefix": "asyncfn",
    "body": [
      "\\$${1:future} = async(${2:10 + 20})",
      "$0"
    ],
    "description": "Async function call"
  },
  "Map Literal": {
    "prefix": "map",
    "body": [
      "\\$${1:config} = {",
      "\\t\"${2:key}\": \"${3:value}\"$0",
      "}"
    ],
    "description": "Create a map literal"
  },
  "Try Catch": {
    "prefix": "try",
    "body": [
      "try {",
      "\\t${1:// code}",
      "} catch (\\$${2:e}) {",
      "\\tprint(\"Error: \" + \\$${2:e})",
      "}"
    ],
    "description": "Try-catch block"
  },
  "Class with Inheritance": {
    "prefix": "classext",
    "body": [
      "class ${1:ChildClass} extends ${2:ParentClass} {",
      "\\tInit constructor(${3:params}) {",
      "\\t\\t$0",
      "\\t}",
      "}"
    ],
    "description": "Class with inheritance"
  },
  "System Run": {
    "prefix": "sysrun",
    "body": [
      "System.Run(\"${1:command}\", [${2:args}])"
    ],
    "description": "Execute system command"
  },
  "System Env": {
    "prefix": "sysenv",
    "body": [
      "\\$${1:var} = System.env(\"${2:KEY}\")"
    ],
    "description": "Get environment variable"
  },
  "SMTP Send": {
    "prefix": "mailsend",
    "body": [
      "\\$smtp = new SmtpClient()",
      "\\$smtp.auth(\"${1:user}\", \"${2:pass}\")",
      "\\$smtp.send(\"${3:to}\", \"${4:subject}\", \"${5:body}\")"
    ],
    "description": "Send email via SMTP"
  },
  "Cron Schedule": {
    "prefix": "cron",
    "body": [
      "Cron.schedule(\"${1:name}\", \"${2:00:00}\", {",
      "\\t$0",
      "})"
    ],
    "description": "Schedule a cron job"
  },
  "Task Hit": {
    "prefix": "task",
    "body": [
      "Task.on_request(\"${1:name}\", \"${2:00:00}\", {",
      "\\t$0",
      "})"
    ],
    "description": "Schedule a hit-based task"
  },
  "View Render": {
    "prefix": "view",
    "body": [
      "return View::render(\"${1:viewName}\", {${2:data}})"
    ],
    "description": "Render a view"
  },
  "TOON Encode": {
    "prefix": "toonenc",
    "body": [
      "\\$toon = toon_encode(${1:data})"
    ],
    "description": "Encode to TOON"
  },
  "TOON Decode": {
    "prefix": "toondeco",
    "body": [
      "\\$data = toon_decode(${1:string})"
    ],
    "description": "Decode TOON string"
  },
  "JSON Encode": {
    "prefix": "jsonenc",
    "body": [
      "\\$json = json_encode(${1:data})"
    ],
    "description": "Encode to JSON"
  },
  "JSON Decode": {
    "prefix": "jsondeco",
    "body": [
      "\\$data = json_decode(${1:string})"
    ],
    "description": "Decode JSON string"
  },
  "Pipe Operator": {
    "prefix": "pipe",
    "body": [
      "${1:value} |> ${2:function}"
    ],
    "description": "Pipe operator - pass value to function"
  },
  "Pipe Chain": {
    "prefix": "pipechain",
    "body": [
      "${1:value} |> ${2:func1} |> ${3:func2} |> ${4:func3}"
    ],
    "description": "Chain multiple functions with pipe operator"
  },
  "Pipe with Args": {
    "prefix": "pipeargs",
    "body": [
      "${1:value} |> ${2:function}(${3:arg})"
    ],
    "description": "Pipe with additional arguments"
  },
  "Lambda Function": {
    "prefix": "lambda",
    "body": [
      "func(${1:$x}) { return ${2:$x * 2}; }"
    ],
    "description": "Anonymous lambda function"
  },
  "Pipe Lambda": {
    "prefix": "pipelambda",
    "body": [
      "${1:value} |> func(${2:$x}) { return ${3:$x}; }"
    ],
    "description": "Pipe to lambda function"
  },
  "Channel Create": {
    "prefix": "chan",
    "body": [
      "\\$${1:ch} = make_chan(${2:0})"
    ],
    "description": "Create a channel"
  },
  "Channel Send": {
    "prefix": "chansend",
    "body": [
      "\\$${1:ch} << ${2:value}"
    ],
    "description": "Send value to channel"
  },
  "Channel Receive": {
    "prefix": "chanrecv",
    "body": [
      "\\$${1:value} = recv(\\$${2:ch})"
    ],
    "description": "Receive value from channel"
  },
  "Channel Foreach": {
    "prefix": "chanfor",
    "body": [
      "foreach (\\$${1:ch} as \\$${2:value}) {",
      "\\t$0",
      "}"
    ],
    "description": "Iterate over channel values"
  },
  "Async with Channel": {
    "prefix": "asyncchan",
    "body": [
      "\\$${1:ch} = make_chan()",
      "\\$${2:future} = async({",
      "\\t\\$${1:ch} << ${3:value}",
      "})",
      "$0"
    ],
    "description": "Async task with channel communication"
  },
  "Schema Create": {
    "prefix": "schema:create",
    "body": [
      "Schema::create(\"${1:tableName}\", func(\\$table) {",
      "\t\\$table.id()",
      "\t\\$table.string(\"${2:name}\")",
      "\t\\$table.timestamps()",
      "})"
    ],
    "description": "Create a new table schema"
  },
  "Schema Drop": {
    "prefix": "schema:drop",
    "body": [
      "Schema::drop(\"${1:tableName}\")"
    ],
    "description": "Drop a table"
  },
  "Schema Drop If Exists": {
    "prefix": "schema:dropIfExists",
    "body": [
      "Schema::dropIfExists(\"${1:tableName}\")"
    ],
    "description": "Drop a table if it exists"
  },
  "Schema Table": {
    "prefix": "schema:table",
    "body": [
      "Schema::table(\"${1:tableName}\", func(\\$table) {",
      "\t\\$table.string(\"${2:column}\")",
      "\t$0",
      "})"
    ],
    "description": "Modify an existing table"
  },
  "Schema Rename": {
    "prefix": "schema:rename",
    "body": [
      "Schema::rename(\"${1:from}\", \"${2:to}\")"
    ],
    "description": "Rename a table"
  },
  "Migration Class": {
    "prefix": "migration",
    "body": [
      "class ${1:CreateUsersTable} extends Migration {",
      "\tfunc up() {",
      "\t\tSchema::create(\"${2:users}\", func(\\$table) {",
      "\t\t\t\\$table.id()",
      "\t\t\t\\$table.string(\"name\")",
      "\t\t\t\\$table.timestamps()",
      "\t\t})",
      "\t}",
      "",
      "\tfunc down() {",
      "\t\tSchema::dropIfExists(\"${2:users}\")",
      "\t}",
      "}"
    ],
    "description": "Create a full migration class"
  },
  "Keys": {
    "prefix": "keys",
    "body": [
      "keys(${1:map})"
    ],
    "description": "Get map keys"
  },
  "Values": {
    "prefix": "values",
    "body": [
      "values(${1:map})"
    ],
    "description": "Get map values"
  },
  "Env": {
    "prefix": "env",
    "body": [
      "env(\"${1:KEY}\")"
    ],
    "description": "Get environment variable"
  },
  "Import": {
    "prefix": "import",
    "body": [
      "import \"${1:path/to/file.joss}\""
    ],
    "description": "Import a file"
  },
  "Auth Validate": {
    "prefix": "auth:validate",
    "body": [
      "\\$valid = Auth::validateToken(${1:\\$token})"
    ],
    "description": "Validate a Bearer token"
  }
}

class Main {
    Init main() {
        print("=== TEST: JWT Token Generation ===")
        print("")
        
        $auth = new Auth()
        
        print("Creando usuario de prueba...")
        string $email = "jwt_test@example.com"
        string $pass = "testPass123"
        $data = [$email, $pass, "JWT Test User"]
        $auth.create($data)
        
        print("")
        print("Generando JWT token...")
        $token = $auth.attempt($email, $pass)
        
        print("")
        print("JWT Token generado:")
        print($token)
        
        print("")
        print("Verificacion:")
        print("- El token debe tener 3 partes separadas por puntos")
        print("- Formato: header.payload.signature")
        print("- Debe ser un string largo (no un numero simple)")
        
        print("")
        print("=== TEST COMPLETADO ===")
    }
}
